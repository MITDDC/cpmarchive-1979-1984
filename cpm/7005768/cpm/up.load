;5:55am  Wednesday, 3 December 1980, GZ@MC
(COMMENT)
(PROGN
 (ERRSET	;make sure user doesn't get stuck in lisp due to lisp error
  (progn	;such as illegal filename.
   (CLOSE (PROG1 INFILE (INPUSH -1)))
   (NOINTERRUPT T)
   (SETQ JCL (IMPLODE (DELETE (ASCII 13.) (CDR (MEMQ '/ (STATUS JCL))))))
   (open tyi '(in image single tty))
   (cursorpos 'A) (princ '|Ready|)(terpri)
   (SETQ OUTSTREAM (OPEN JCL '(OUT ascii))); ascii mode causes CR==>CRLF
   (DO ((C (TYI) (TYI)) (cnt 0))
       ((and (= cnt 9.) (= c 3)))
       (cond ((= c 3) (setq cnt (1+ cnt)))(t (setq cnt 0)))
       (TYO C OUTSTREAM))
   (cursorpos 'A)
   (princ '|Done|)
   (close OUTSTREAM))
  T)	;print out error messages, why not
 (quit))
